<%= form_for(@song) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :release_year %>
  <%= f.number_field :release_year %>

  <%= f.label :released %>
  <%= f.check_box :released %>

  <%= f.label :genre %>
  <%= f.number_field :genre %>

  <%= f.label :artist_name %>
  <%= f.number_field :artist_name %>

  <%= f.submit %>
<% end %>
